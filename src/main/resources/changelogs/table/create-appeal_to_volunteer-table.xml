<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <changeSet id="create-appeal_to_volunteer-table" author="Evgeniy Zinchenko">
        <preConditions onFail="WARN">
            <not>
                <tableExists tableName="appeal_to_volunteer"/>
            </not>
        </preConditions>

        <createTable tableName="appeal_to_volunteer">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" />
            </column>
            <column name="user_id" type="BIGINT">
                <constraints nullable="false" foreignKeyName="fk_user1" references="&quot;user&quot;(&quot;user_id&quot;)"/>
            </column>
        </createTable>


    </changeSet>

</databaseChangeLog>