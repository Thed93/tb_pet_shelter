<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <changeSet id="create-help-table.4" author="Evgeniy Zinchenko">
        <preConditions>
            <not>
                <tableExists tableName="help"/>
            </not>
        </preConditions>

        <createTable tableName="help">
            <column name="id" autoIncrement="true" type="BIGINT">
                <constraints primaryKey="true" />
            </column>
            <column name="user_id" type="BIGINT">
                <constraints nullable="false" foreignKeyName="fk_user2" references="&quot;user&quot;(&quot;user_id&quot;)"/>
            </column>
            <column name="text" type="VARCHAR(20)">
                <constraints nullable="false"/>
            </column>

        </createTable>


    </changeSet>

</databaseChangeLog>